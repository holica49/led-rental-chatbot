{
  "name": "led-rental-mcp",
  "version": "0.1.0",
  "description": "A Model Context Protocol server for LED rental automation",
  "type": "module",
  "bin": {
    "led-rental-mcp": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "mcp.json"
  ],
  "scripts": {
    "start": "tsx src/index.ts",
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
